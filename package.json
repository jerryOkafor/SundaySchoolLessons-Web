{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-router-dom": "^4.2.2",
    "firebase": "^4.11.0",
    "firebaseui": "^2.6.2",
    "hoek": "^5.0.3",
    "immutability-helper": "^2.6.6",
    "latinize": "^0.2.0",
    "material-ui": "^0.20.0",
    "sweetalert2": "^7.18.0"
  },
  "scripts": {
    "start": "REACT_APP_DIST_ENV=staging  react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --bail --env=jsdom --runInBand",
    "reportCoverage": "codecov",
    "test:config": "yarn test --showConfig",
    "test:coverage": "yarn test --coverage --silent --updateSnapshot",
    "validate:dependencies": "nsp check",
    "eject": "react-scripts eject",
    "deploy:staging": "yarn build && firebase deploy -P default",
    "build:production": "REACT_APP_DIST_ENV=production yarn build",
    "deploy:production": "yarn build && firebase deploy -P prod"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "firebase-tools": "^3.18.4",
    "nsp": "^3.2.1"
  }
}